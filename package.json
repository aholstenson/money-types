{
  "name": "money-types",
  "version": "0.1.0",
  "description": "Types for representing and manipulating money",
  "scripts": {
    "test": "jest",
    "ci": "npm run coverage && npm run lint",
    "coverage": "jest --coverage",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "build": "tsc --module commonjs --target es5 --outDir dist/cjs && tsc --module es6 --target es6 --outDir dist/esm",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "repository": "aholstenson/money-types",
  "keywords": [],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "numeric-types": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.4",
    "cldr-core": "^35.0.0",
    "cldr-numbers-modern": "^35.0.0",
    "coveralls": "^3.0.3",
    "jest": "^24.7.1",
    "jest-config": "^24.7.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  }
}
